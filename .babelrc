{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-react"
  ],

  "plugins": [
    "transform-class-properties",
    "@babel/plugin-transform-arrow-functions"
  ],
  "env": {
    "test": {
      "presets": ["@babel/env", "@babel/react"]
    }
  }
}
